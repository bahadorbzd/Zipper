{
    "name": "bahadorbzd/zipper",
    "type": "library",
    "description": "This is a little neat helper for the ZipArchive methods with handy functions",
    "keywords": ["laravel", "ZIP", "Archive"],
    "homepage": "http://github.com/bahadorbzd/zipper",
    "license": "MIT",
    "authors": [
        {
            "name": "bahador babazadeh",
            "email": "email.bahador@gmail.com",
            "homepage": "http://bahadorbzd.ir",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "ext-zip": "*",
        "illuminate/support": "^6|^7|^8|^9",
        "illuminate/filesystem": "^8.42|^9.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5.5",
        "mockery/mockery": "^1.3"
    },
    "autoload": {
        "psr-4": {
            "Chumper\\Zipper\\": "src/Chumper/Zipper"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Chumper\\Zipper\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Chumper\\Zipper\\ZipperServiceProvider"
            ],
            "aliases": {
                "Zipper": "Chumper\\Zipper\\Zipper"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}